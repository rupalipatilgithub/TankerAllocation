<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardBackgroundColor="@color/sweet_white"
    app:cardCornerRadius="8dp"
    app:cardPreventCornerOverlap="true"
    app:cardUseCompatPadding="true">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/sweet_white"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/backgroud_viewer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:id="@+id/card_token_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_marginTop="0dp"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/tv_toker_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:text="@string/token_no"
                    android:textColor="@color/stellar_explorer" />

                <TextView
                    android:id="@+id/val_token_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:textColor="@color/stellar_explorer" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/card_cust_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/card_token_no"
                android:layout_margin="5dp"
                android:layout_marginTop="0dp"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/tv_cust_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:text="@string/Cust_name"
                    android:textColor="@color/stellar_explorer" />

                <TextView
                    android:id="@+id/val_cus_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:textColor="@color/stellar_explorer" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/card_building_no"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/card_cust_name"
                android:layout_margin="5dp"
                android:layout_marginTop="0dp"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/tv_building_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:text="@string/buil_name"
                    android:textColor="@color/stellar_explorer" />

                <TextView
                    android:id="@+id/val_buil_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:textColor="@color/stellar_explorer" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/card_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/card_building_no"
                android:layout_margin="5dp"
                android:layout_marginTop="0dp"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/tv_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:text="@string/deliverydate"
                    android:textColor="@color/stellar_explorer" />

                <TextView
                    android:id="@+id/val_deiverydate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:textColor="@color/stellar_explorer" />


            </LinearLayout>


           <!-- <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:layout_below="@+id/card_date"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/uloaddelivery_note"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:textColor="@color/white"
                    app:cornerRadius="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="@string/uploaddelivery_note"
                    android:layout_gravity="right"
                    android:layout_marginRight="15dp"/>


            </LinearLayout>
-->


        </RelativeLayout>
    </RelativeLayout>

</androidx.cardview.widget.CardView>


